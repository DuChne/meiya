/* ywsq_sun/pages/consultationRecord/index.wxss */
.consultation-record{
  box-shadow: 0 0 3rpx 0 #BFBFBF;
  width: 700rpx;
  height: 156rpx;
  border-radius: 15rpx;
  margin: 0 auto;
  box-sizing: border-box;
  padding-top: 28rpx;
  margin-bottom: 24rpx;
}
.record-time{
  display: flex;
  font-size: 22rpx;
  line-height: 1em;
  padding-bottom: 18rpx;
}
.record-time .title{
  width: 160rpx;
  padding-left: 24rpx;
  color: #212121;
}
.record-time .details{
  color: #555555;
}
.no-more-history{
  color: #BFBFBF;
  font-size: 25rpx;
  position: relative;
  text-align: center;
  line-height: 90rpx;
}
.no-more-history::after{
  position: absolute;
  right: 25rpx;
  top: 44rpx;
  width: 250rpx;
  border-bottom: 1rpx solid #BFBFBF;
  content: "";
  display: block;
}
.no-more-history::before{
  position: absolute;
  left: 25rpx;
  top: 44rpx;
  width: 250rpx;
  border-bottom: 1rpx solid #BFBFBF;
  content: "";
  display: block;
}