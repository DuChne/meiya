    height: 80rpx;
    line-height: 80rpx;
    font-size: 28rpx;
}

.joinCart {
    background-color: #FFA09A;
    border-top-left-radius: 40rpx;
    border-bottom-left-radius: 40rpx;
    margin-left: 50rpx;
}

.buyNow {
    background-color: #DF463C;
    border-top-right-radius: 40rpx;
