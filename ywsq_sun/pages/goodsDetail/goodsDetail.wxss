    border-radius: 100%;
    color: #fff;
    text-align: center;
}

.guigeBox {
    position: fixed;
    left: 0;
    bottom: 0;
    width: 100%;
    background-color: #fff;
    z-index: 13;
}

.closeBox {
    display: flex;
    justify-content: flex-end;
}

.close {
    width: 28rpx;
    height: 28rpx;
    margin: 30rpx;
}

.guigeMes {
    padding: 0 20rpx 30rpx;
}

.goodsImg {
    width: 100%;
    height: 376rpx;
}

.guigeP {
    font-size: 28rpx;
    color: #fb0303;
    line-height: 28rpx;
    margin: 30rpx 0 20rpx;
}

.remainNum {
    font-size: 24rpx;
    color: #999;
}

.size-label-box {
    padding: 20rpx;
}

.size-label-box .label {
    font-size: 26rpx;
    line-height: 26rpx;
    color: #000;
    margin: 0 0 20rpx 0;
}

.size-label-box .label-item-box {
    display: flex;
    flex-direction: row;
    flex-wrap: wrap;
}

.size-label-box .label-item {
    color: #666;
    font-size: 24rpx;
    background-color: #f3f3f3;
    padding: 6rpx 10rpx;
    height: 40rpx;
    border-radius: 20rpx;
    text-align: center;
    line-height: 40rpx;
    margin: 0 20rpx 10rpx 0;
}

.size-label-box .label-item.active {
    color: #fff;
    background-color: #ce0000;
}

.buyBox {
    padding: 30rpx 20rpx;
}

.num-box {
    display: flex;
    z-index: 9;
}

.num-box .num-jian,.num-box .num-input,.num-box .num-jia {
    width: 66rpx;
    height: 46rpx;
    line-height: 46rpx;
    text-align: center;
    border: 1px solid #eee;
    font-size: 40rpx;
    margin: 3rpx;
}

.num-box .num-jian,.num-box .num-jia {
    background-color: #fff;
}

.num-box .num-jian.disabled,.num-box .num-jia.disabled {
    background-color: #f5f5f9;
}

.num-box .num-input {
    font-size: 28rpx;
}

.num-box .num-input input {
    height: 100%;
}

.deterBtn {
    width: 450rpx;
    height: 80rpx;
