<view class="sec-footer" style="background:{{color.foot_color}}">
    <view bindtap="_onNavTab" class="item" data-index="{{index}}" wx:for="{{nav}}" wx:key="{{index}}">
        <image src="{{item.choose===0?item.img:item.imgh}}"></image>
        <text style="color:{{item.choose===0?color.foot_font_color_one:color.foot_font_color_two}}">{{item.txt}}</text>
    </view>
</view>
